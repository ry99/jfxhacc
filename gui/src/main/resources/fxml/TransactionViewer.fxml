						<TableColumn fx:id="date" editable="false" prefWidth="75.0" text="Date" />
						<TableColumn fx:id="number" editable="false" prefWidth="75.0" text="Number" />
						<TableColumn fx:id="payee" editable="false" prefWidth="255.0" text="Payee/Account/Memo" />
						<TableColumn fx:id="increase" editable="false" minWidth="0.0" prefWidth="67.0" text="Deposit" />
						<TableColumn fx:id="decrease" editable="false" minWidth="5.0" prefWidth="73.0" text="Withdrawal" />
						<TableColumn fx:id="reco" editable="false" prefWidth="52.0" text="Rec" />
                  <TableColumn fx:id="jcol" editable="false" prefWidth="75.0" text="Journal" />
