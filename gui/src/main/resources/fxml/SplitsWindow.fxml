<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="275.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button cancelButton="true" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
          <Button defaultButton="true" mnemonicParsing="false" onAction="#save" text="OK" />
        </buttons>
      </ButtonBar>
   </bottom>
   <center>
      <TableView fx:id="splittable" editable="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="account" prefWidth="200.0" text="Account" />
            <TableColumn fx:id="memo" prefWidth="150.0" text="Memo" />
          <TableColumn fx:id="credit" prefWidth="75.0" text="Credit" />
            <TableColumn fx:id="debit" prefWidth="75.0" text="Debit" />
            <TableColumn fx:id="reco" prefWidth="50.0" text="Rec" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
